{{define "title"}}Home{{end}}
{{define "main"}}
    <h2>My first tracked swim</h2>
    <div class="swim-track">
        <div>
            <h3>Date</h3>
            <p>{{ .Swim.Date }}</p>
        </div>
        <div>
            <h3>Distance</h3>
            <p>{{ .Swim.DistanceM }}m</p>
        </div>
        <div>
            <h3>Assessment</h3>
            <p>{{ .Swim.Assessment }}</p>
        </div>
    </div>
{{end}}

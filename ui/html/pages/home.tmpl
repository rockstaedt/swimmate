{{define "title"}}Home{{end}}
{{define "main"}}
    <div class="dashboard">
        <div class="dashboard-entry">
            <h3>Total Swims</h3>
            <p>{{ .Data.TotalCount }}</p>
        </div>
        <div class="dashboard-entry">
            <h3>Total Distance</h3>
            <p>{{ .Data.TotalDistance }}</p>
        </div>
        <div class="dashboard-entry">
            <h3>Monthly Count</h3>
            <p>{{ .Data.MonthlyCount}}</p>
        </div>
        <div class="dashboard-entry">
            <h3>Monthly Distance</h3>
            <p>{{ .Data.MonthlyDistance }}</p>
        </div>
        <div class="dashboard-entry">
            <h3>Weekly Count</h3>
            <p>{{ .Data.WeeklyCount }}</p>
        </div>
        <div class="dashboard-entry">
            <h3>Weekly Distance</h3>
            <p>{{ .Data.WeeklyDistance }}</p>
        </div>
    </div>
{{end}}
